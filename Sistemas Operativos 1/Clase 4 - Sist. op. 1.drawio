        <mxCell id="jdEvVsPho3DZVX9i4UFz-8" value="Interrupciones" style="rounded=0;whiteSpace=wrap;html=1;fillColor=#a20025;fontColor=#ffffff;strokeColor=#6F0000;fontSize=14;" vertex="1" parent="1">
